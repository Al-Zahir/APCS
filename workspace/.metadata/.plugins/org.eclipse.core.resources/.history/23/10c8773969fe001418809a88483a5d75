package com.Zeditude.APCompFinal;

import java.util.List;

public class Node {
	private Board currentBoard;
	//private List<Node> nodes;
	
	public Node(Board b){
		currentBoard = b;
	}
	
	public Board getBoard(){
		return currentBoard;
	}
	
	public String toString(){
		String s = "";
		
		for(int r = 0; r < currentBoard.getBoard().length; r++)
			for(int c = 0; c < currentBoard.getBoard()[r].length; c++)
				s += currentBoard.getBoard()[r][c].getType() + " ";
		
		return s;
	}
}
